{
  "name": "bookclub-stats",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://mknabe.github.io/book-club",
  "dependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.10",
    "antd": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.0",
    "typescript": "^4.9.4",
    "wouter": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
